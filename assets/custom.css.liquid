/* product page */
.product-banner .product-image{
  position:relative;
}
.product-banner .product-image .product-desc{
  position:absolute;
  top:50%;
  left:75px;
  z-index:1;
  transform: translateY(-50%);
} 
.product-banner .product-image .product-desc h1{
  font-size:62px;
  color:#fff;
  text-transform:uppercase;
}
.product-banner .product-image .product-desc h1 sup{
  font-size:22px;
  top: -17px;
  position: relative;
  font-weight:300;
}
.product-information-new .product-thumbnail-gallery ul{
  list-style:none;
  padding:0;
  display:flex;
  flex-wrap:wrap;
}
.product-information-new .product-thumbnail-gallery ul li{
  margin:0 5px; 
  width:calc(33.3% - 10px);
  padding:0 5px;
  min-height:200px;
}
.product-information-new .product-thumbnail-gallery ul li img{
  width:100%;
  height:100%;
}
.product-information-new .grid{
  column-gap: 65px;
}
.product-information-new .grid__item.product-left{
  width: calc(50% - 65px);
}
.product-information-new .product-right h2{
  font-size:32px;
  font-weight:600;
  line-height:40px;
  text-transform:uppercase;
  color:#202020;
  margin-bottom:10px;
}
.product-information-new .product-right .p-decription{
  line-height:30px; 
  padding-top:20px;
  padding-bottom:28px;
}
.product-information-new .product-right .feature-benifits ul,
.product-information-new .product-right .key-specification ul{
  list-style:disc;
  padding: 0 15px;
}
.product-information-new .product-right .feature-benifits ul li,
.product-information-new .product-right .key-specification ul li{
  line-height:31px;
}
.product-information-new .product-right .key-specification ul{margin-bottom:20px;}
.product-information-new .product-right .key-specification ul li span{
  font-weight:500;
}
.product-information-new .product-right .products-button{
  margin:50px 0 40px;
}
.variant-ProductOptionDrop li{
  margin:0 7px;
}
.product-thumbnail-gallery .swiper-slide svg{
  width:24px;
  position:absolute;
  top:50%;
  left:50%;
  transform: translate(-50%, -50%);
}
.product-thumbnail-gallery .swiper-slide img{
  height: 90px;
  width: 100%;
  object-fit: cover;
}
.product-thumbnail-gallery .swiper-button-next, .product-thumbnail-gallery .swiper-button-prev{
  display:block;
  width: 15px;
  height: 15px;
}
.product-thumbnail-gallery .swiper-button-next:after, 
.product-thumbnail-gallery .swiper-button-prev:after{
font-size:20px;
  color:#fff;
}
.product-thumbnail-gallery .swiper-button-next:hover:after, 
.product-thumbnail-gallery .swiper-button-prev:hover:after{
  color:#000;
}

@media(max-width:767px){
  .product-banner .product-image .product-desc h1{
