/* ======== header ======== */
.header{
  position: absolute;
  top: 0;
  width: 100%;
  z-index: 9;
  border-bottom: 1px solid #262626;
  padding-top: 1rem;
  padding-bottom: 1rem;
}
.button.button--primary{
  background:transparent;
  padding:4px 10px;
  color:#202020;
  font-size:12px;
  text-transform:uppercase;
  letter-spacing:2px;
  font-weight:400;
  font-family: 'RM Neue TRIAL';
  opacity:1;
  position:relative;
  z-index:1;
}
.button.button--primary:before{
  position:absolute;
  left:0;
  width:45px;
  height:45px;
  border:2px solid #323232;
  background:transparent;
  content:"";
  z-index:-1;
} 
.button.button--primary:after{
  box-shadow:none !important;
  background:#323232;
  width:28px;
  height:2px;
  position:absolute;
  right:-30px;
  top:50%;
  transform:translateY(-50%);
  content:"";
  z-index:1;
  left: unset;
  border: none;
}
.button.button--primary span{
  padding:7px 10px;
  background:#fff;
}
.image-banner .button.button--primary span{
  background:#b9e4eb;
}
.button.button--blackbg{
  background:#202020;
  padding:2px 68px;
  color:#fff;
  font-size:18px;
  text-transform:uppercase;
  letter-spacing:0px;
  font-weight:400;
  font-family: 'RM Neue TRIAL';
  opacity:1;
  position:relative;
  z-index:1;
  border:1px solid #202020;
}
.button.button--blackbg svg{
  color:#fff;
  fill:#fff;
}
.button.button--blackbg:hover{
  background:transparent;
  color:#202020;
  border:1px solid #202020;
}
.button.button--blackbg:hover svg{
  fill:#202020;
  color:#202020;
}
.button.button--blackbg:hover:after,
.button.button--primary-border:hover:after,
.button.button--primary-border:after{
  box-shadow:none;
}
.button.button--primary-border{
  background:transparent;
  padding:2px 68px;
  color:#202020;
  font-size:18px;
  text-transform:uppercase;
  letter-spacing:0px;
  font-weight:400;
  font-family: 'RM Neue TRIAL';
  opacity:1;
  position:relative;
  z-index:1;
  border:2px solid #202020;
}
.button.button--primary-border:hover{
  background:#202020;
  color:#fff;
  border:1px solid #202020;
}
.button.button--primary-border:hover svg{    
  color: #fff;
  fill: #fff;
}
.button.button--secondary{
  text-transform:uppercase;
  background:transparent;
  font-size:20px;
  font-weight:300;
  color:#fff;
  text-decoration:underline;
  border:none;
  padding:0;
  letter-spacing:0;
  text-underline-offset: 3px;
  text-decoration-thickness: 1px;
}
.button.button--secondary:before,
.button.button--secondary:after{
  box-shadow:none;
  display:none;
}
.button.button--secondary:hover{
  border:none;
  text-decoration:none;
}
 

@media(max-width:767px){
  .button.button--blackbg,
  .button.button--primary-border{
    font-size:15px;
    width:100%;
  }
  .button.button--secondary{
    font-size:18px;
  }
 
}

@media(min-width:990px){
  .content-for-layout{
  margin-top:98px;
  }
  body.index .content-for-layout{
  margin-top:0;
  }
}
@media(min-width:768px){
  .product .button.button--blackbg,
  .product .button.button--primary-border{
    min-width:381px;
  }
  body.index .header:after{
  content: "";
  right: 0;
  width: calc(100% - 50%);
  z-index: 1;
  background: #d6d6d6;
  height: 1px;
  bottom: -1px;
  position: absolute;
}
}

.image-with-text .image-with-text__content .image-with-text__text+.button{
  margin-top:55px;
}
@media(max-width:992px){
  .header{
    position:unset;
  }
  .image-with-text .image-with-text__content .image-with-text__text+.button{
    margin-top:35px;
  }
}


